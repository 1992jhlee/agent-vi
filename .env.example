# Database
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/agent_vi
DATABASE_URL_SYNC=postgresql+psycopg2://postgres:postgres@localhost:5432/agent_vi

# DART OpenAPI (https://opendart.fss.or.kr)
DART_API_KEY=your_dart_api_key_here

# Naver Developers (https://developers.naver.com)
NAVER_CLIENT_ID=your_naver_client_id_here
NAVER_CLIENT_SECRET=your_naver_client_secret_here

# YouTube Data API v3 (Google Cloud Console)
YOUTUBE_API_KEY=your_youtube_api_key_here

# LLM Providers
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Default LLM model (litellm format)
DEFAULT_LLM_MODEL=gpt-4o

# Frontend URL (for ISR revalidation webhook)
FRONTEND_URL=http://localhost:3000
REVALIDATION_SECRET=your_revalidation_secret_here

# Backend
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8000
